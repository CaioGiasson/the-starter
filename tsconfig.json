{
	"compilerOptions": {
		"target": "es2016",
		"lib": ["DOM"],
		"module": "CommonJS",
		"rootDir": "src",
		"resolveJsonModule": true,
		"outDir": "build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	}
}
